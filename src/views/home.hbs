<div class="container">
    <h1>Lista de productos </h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Nombre</th>
                <th scope="col">Descripción</th>
                <th scope="col">Código</th>
                <th scope="col">Precio</th>
                <th scope="col">Stock</th>
                <th scope="col">Categoria</th>
            </tr>
        </thead>
        <tbody>
            {{#each products}}
                <tr>
                    <td> {{this.id}}            </td>
                    <td> {{this.title}}         </td>
                    <td> {{this.description}}   </td>
                    <td> {{this.code}}          </td>
                    <td> {{this.price}}         </td>
                    <td> {{this.stock}}         </td>
                    <td> {{this.category}}      </td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>
